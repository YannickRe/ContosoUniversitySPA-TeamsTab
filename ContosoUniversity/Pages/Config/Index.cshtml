@page
@model IndexModel
@{ ViewData["Title"] = "Config"; }

<h1>@ViewData["Title"]</h1>

<p>This tab does not require configuration.</p>

@section Scripts {
    <script>
        microsoftTeams.settings.setValidityState(true);
        microsoftTeams.settings.registerOnSaveHandler((saveEvent) => {
            microsoftTeams.settings.setSettings({
                websiteUrl: "https://4x10.azurewebsites.net/",
                contentUrl: "https://4x10.azurewebsites.net/SSO",
                entityId: "contosoUniversity",
                suggestedDisplayName: "Contoso University",
                removeUrl: ""
            });
            saveEvent.notifySuccess();
        });
    </script>
}